        }
      },
      {
        "id": 5765893725785476,
        "definition": {
          "type": "timeseries",
          "title": "Chat Success Rate (%) / Error Rate (%)",
          "title_size": "16",
          "title_align": "left",
          "show_legend": true,
          "requests": [
            {
              "response_format": "timeseries",
              "queries": [
                {
                  "name": "success",
                  "data_source": "metrics",
                  "query": "sum:dd_gemini.slo_chat_success_count{service:dd-gemini-app,endpoint:chat}.as_count()"
                },
                {
                  "name": "errors",
                  "data_source": "metrics",
                  "query": "sum:dd_gemini.slo_chat_error_count{service:dd-gemini-app,endpoint:chat}.as_count()"
                }
              ],
              "formulas": [
                {
                  "alias": "Success Rate (%)",
                  "formula": "100 * success / (success + errors)"
                },
                {
                  "alias": "Error Rate (%)",
                  "formula": "100 * errors / (success + errors)"
                }
              ],
              "display_type": "line"
            }
          ],
          "yaxis": {
            "include_zero": true,
            "min": "0",
            "max": "100"
          }
        }
      },
